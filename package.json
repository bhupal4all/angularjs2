{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "build:sass": "node-sass --output . --recursive .",
    "watch:sass": "node-sass --output . --recursive --watch .",
    "postinstall":"npm build"
  },
  "scripts": {
  "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
  "heroku-postbuild": "echo This runs afterwards."
},
"engines": {
  "node":"~6.10.3",
  "npm":"~3.10.5"
},
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "systemjs": "0.19.22",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15",
          "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "node-sass": "^4.3.0",
    "typescript": "^1.7.5",
    "typings": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "node-sass": "^4.3.0",
